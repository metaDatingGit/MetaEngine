/* AS3	Copyright 2012 Philipp Schoessler*/package {		public class ProjectObject {				//--------------------------------------//  PRIVATE VARIABLES//--------------------------------------		private var vidId:int;		private var clipName:String;		private var proContra:String;		private var reactionAction:String;		private var maleFemale:String;		private var strategy:String		private var intensity:int;		private var shot:String;		private var clipPos:String;		private var family:int;		private var religion:int;		private var emotion:int;		private var society:int;		private var routine:int;		private var spareTime:int;		private var safety:int;		private var freedom:int;		private var loyaltySex:int;		private var emotionalReaction:String;		private var flowingReaction:String; 		private var joker:String;		//--------------------------------------//  CONSTRUCTOR//--------------------------------------		public function ProjectObject(_vidId:int,									   _clipName:String,									   _proContra:String,									   _reactionAction:String,									   _maleFemale:String,									   _strategy:String,									   _intensity:int,									   _shot:String,									   _clipPos:String,									   _family:int,									   _religion:int,									   _emotion:int,									   _society:int,									   _routine:int,									   _spareTime:int,									   _safety:int,									   _freedom:int,									   _loyaltySex:int,									   _emotionalReaction:String,									   _flowingReaction:String,									   _joker:String 									   ) 		{			vidId = _vidId;			clipName = _clipName;			proContra = _proContra;			reactionAction = _reactionAction;			maleFemale = _maleFemale;			strategy = _strategy;			intensity = _intensity;			shot = _shot;			clipPos = _clipPos;			//tags			family = _family;			religion = _religion;			emotion = _emotion;			society = _society;			routine = _routine;			spareTime = _spareTime;			safety = _safety;			freedom = _freedom;			loyaltySex = _loyaltySex;			emotionalReaction = _emotionalReaction;			flowingReaction = _flowingReaction;			joker = _joker;		}				//--------------------------------------//  GETTERS//--------------------------------------				public function get getVidId():int{			return vidId;		}				public function get getClipName():String{			return clipName;		}				public function get getProContra():String{			return proContra;		}				public function get getReactionAction():String{			return reactionAction;		}				public function get getMaleFemale():String{			return maleFemale;		}				public function get getStrategy():String{			return strategy;		}				public function get getIntensity():int{			return intensity;		}				public function get getShot():String{			return shot;		}				public function get getClipPos():String{			return clipPos;		}				public function get getFamily():int{			return family;		}				public function get getReligion():int{			return religion;		}				public function get getEmotion():int{			return emotion;		}				public function get getSociety():int{			return society;		}				public function get getRoutine():int{			return routine;		}		public function get getSpareTime():int {			return spareTime;		}				public function get getSafety():int {			return safety;		}				public function get getFreedom():int{			return freedom;		}				public function get getLoyaltySex():int{			return loyaltySex;		}				public function get getEmotionalReaction():String{			return emotionalReaction;		}				public function get getFlowingReaction():String{			return flowingReaction;		}				public function get getJoker():String{			return joker;		}	}}